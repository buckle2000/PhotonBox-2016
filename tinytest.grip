      <grip:Output step="1" socket="1" previewed="false"/>
      <grip:Output step="1" socket="2" previewed="false"/>
      <grip:Output step="1" socket="3" previewed="false"/>
    </grip:Step>
    <grip:Step name="Publish Number">
      <grip:Input step="2" socket="0"/>
      <grip:Input step="2" socket="1">
        <value>greyMin</value>
      </grip:Input>
      <grip:Input step="2" socket="2">
        <value>true</value>
      </grip:Input>
    </grip:Step>
    <grip:Step name="Publish Number">
      <grip:Input step="3" socket="0"/>
      <grip:Input step="3" socket="1">
        <value>greyMax</value>
      </grip:Input>
      <grip:Input step="3" socket="2">
        <value>true</value>
